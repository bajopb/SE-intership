        <TextBox x:Name="tbFilteringTime" HorizontalAlignment="Left" Margin="166,316,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="60"/>
        <Label Content="Grinder Method" HorizontalAlignment="Left" Margin="71,74,0,0" VerticalAlignment="Top"/>
        <Label Content="Grinder Time" HorizontalAlignment="Left" Margin="87,127,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.299,-0.894"/>
        <Label Content="Saharification Method" HorizontalAlignment="Left" Margin="39,153,0,0" VerticalAlignment="Top"/>
        <Label Content="Saharification Temperature" HorizontalAlignment="Left" Margin="14,179,0,0" VerticalAlignment="Top"/>
        <Label Content="Saharification Time" HorizontalAlignment="Left" Margin="55,0,0,0" VerticalAlignment="Center"/>
        <Label Content="Filtering Temperature" HorizontalAlignment="Left" Margin="43,286,0,0" VerticalAlignment="Top"/>
        <Label Content="Grinder Temperature" HorizontalAlignment="Left" Margin="46,101,0,0" VerticalAlignment="Top"/>
