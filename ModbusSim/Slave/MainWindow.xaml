        <TextBox KeyDown="tbSetIRGrindingTemperature_KeyDown" x:Name="tbSetIRGrindingTemperature" HorizontalAlignment="Left" Margin="315,105,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="60"/>
        <TextBox KeyDown="tbSetIRGrindingTime_KeyDown" x:Name="tbSetIRGrindingTime" HorizontalAlignment="Left" Margin="315,131,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="60"/>
        <TextBox KeyDown="tbSetIRSaharificationMethod_KeyDown" x:Name="tbSetIRSaharificationMethod" HorizontalAlignment="Left" Margin="315,157,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="60"/>
