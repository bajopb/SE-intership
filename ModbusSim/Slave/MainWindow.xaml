<Window x:Class="Slave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Slave"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnConnect" Content="Button" HorizontalAlignment="Left" Margin="10,68,0,0" VerticalAlignment="Top" Height="40" Width="230" Click="btnConnect_Click"/>
        <Button x:Name="btnConnect_Copy" Content="Button" HorizontalAlignment="Left" Margin="10,113,0,0" VerticalAlignment="Top" Height="40" Width="230" Click="btnConnect_Copy_Click"/>

    </Grid>
</Window>
