<Window x:Class="Master.Views.WriteSingleHoldingRegisterView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Master.Views"
        mc:Ignorable="d"
        Title="WriteSingleHoldingRegisterView" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbUnitId" HorizontalAlignment="Left" Margin="27,47,0,0" TextWrapping="Wrap"   VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbAddress" HorizontalAlignment="Left" Margin="27,91,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbValue" HorizontalAlignment="Left" Margin="27,135,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Content="Unit ID" HorizontalAlignment="Left" Margin="27,21,0,0" VerticalAlignment="Top"/>
        <Label Content="Address" HorizontalAlignment="Left" Margin="27,65,0,0" VerticalAlignment="Top"/>
        <Label Content="Value" HorizontalAlignment="Left" Margin="27,109,0,0" VerticalAlignment="Top"/>
        <Button Content="Write" HorizontalAlignment="Left" Margin="27,158,0,0" VerticalAlignment="Top" Width="120" Height="29" Click="Button_Click"/>

    </Grid>
</Window>
