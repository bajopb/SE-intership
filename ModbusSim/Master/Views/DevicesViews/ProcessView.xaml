<Window x:Class="Master.Views.DevicesViews.ProcessView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Master.Views.DevicesViews"
        mc:Ignorable="d"
        Title="ProcessView" Height="450" Width="800">
    <Grid>
        <TextBox Text="{Binding HoldingRegister}" HorizontalAlignment="Center" Margin="0,147,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Text="{Binding InputRegister}" HorizontalAlignment="Center" Margin="0,182,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>


    </Grid>
</Window>
